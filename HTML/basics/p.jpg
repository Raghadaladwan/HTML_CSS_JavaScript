    (function(){
        if(window.XMLHttpRequest && window.tmg && window.tmg.GET_PARAM && typeof tmg.GET_PARAM.playlist != 'undefined') {

            // inject CSS to hide the content
            var currentCssVersion = '',
                    xmlhttp = new XMLHttpRequest(),
                    headTag = document.querySelector("head"),
                    bodyTag = document.body,
                    loadingDiv, metas = headTag.getElementsByTagName('meta'),
                    videoURL,
                    createElement = function(tag, attrs, innerHTML, parent) {
                        var el = document.createElement(tag);
                        if(typeof attrs == 'object') {
                            for(k in attrs) {
                                el.setAttribute(k, attrs[k]);
                            }
                        }
                        if(typeof innerHTML === 'string' && innerHTML !== '')
                            el.innerHTML = innerHTML;
                        if(typeof parent === 'object')
                            parent.appendChild(el);
                        return el;
                    },
                    removeMetaIfExists = function (key) {
                        // metas is defined outside this function
                        for (var i = 0; i < metas.length; i++) {
                            if (metas[i].getAttribute('name') == key) {
                                headTag.removeChild(metas[i]);
                            }
                        }
                    };

            document.documentElement.className += ' video-loading';

            // video player placeholder DIV with the loading image
            loadingDiv = createElement(
                'div',
                {"id":"loading"},
                '<div class="loading-content"><div class="loading-indicator loading-indicator--mediumBorder">' +
                    '<div class="loading-indicator__cube"></div>' +
                    '<div class="loading-indicator__cube"></div>' +
                    '<div class="loading-indicator__cube"></div>' +
                    '<div class="loading-indicator__cube"></div>' +
                    '<div class="loading-indicator__cube"></div>' +
                '</div></div>', bodyTag
            );

            // append video CSS tag to head
            createElement(
                'link',
                {
                    "href":"/etc/designs/telegraph/core/clientlibs/tmgchannels/video." + currentCssVersion + ".css",
                    "rel":"stylesheet"
                }, '',  headTag
            );

            // remove metas
            removeMetaIfExists('tmgads.pagetype');
            removeMetaIfExists('DCSext.Content_Type');
            // append video meta datas
            createElement('meta', {'name':'tmgads.pagetype', 'content':'video-play'}, '', headTag);
            createElement('meta', {'name':'DCSext.Content_Type', 'content':'video-play'}, '', headTag);

            // Static Overwrites are defined in metaTags
            if(typeof metaStaticOverwrites === 'object') {
                for(key in metaStaticOverwrites) {
                    removeMetaIfExists(key);
                    createElement('meta', {'name':key, 'content':metaStaticOverwrites[key]}, '', headTag);
                }
            }
            if(typeof dataLayerStaticOverwrites === 'object' && typeof dataLayer === 'object') {
                for(key in dataLayerStaticOverwrites) {
                    dataLayer[key] = dataLayerStaticOverwrites[key];
                }
            }

            if(typeof tmgAds === 'object') {
                if(typeof tmgAdsInitData === 'function') {
                    tmgAdsInitData(tmgAds);
                }
                if(typeof tmgAdsInitDFP === 'function') {
                    tmgAdsInitDFP(tmgAds);
                }
            }

            setTimeout(function() {
                loadingDiv.innerHTML =  '<div class="loading-content">' +
                                            '<a href="' + window.location + '"><p>' +
                                                'It looks like something is not quite right<br> with your internet connection.<br> Please refresh the page and retry.' +
                                            '</p></a>' +
                                        '</div>';
            }, 10000);

            // get json
            xmlhttp.onreadystatechange = function() {
                if (xmlhttp.readyState === XMLHttpRequest.DONE ) {
                    // if (status code == 200)
                    if(xmlhttp.status === 200){
                        // Remove old content from DOM
                        var mainContents = document.getElementsByClassName('main-content'),
                            videoPageMainContents;

                        // append new content template
                        var videoPage = createElement('div', {}, xmlhttp.responseText, false);
                        videoPageMainContents = videoPage.getElementsByClassName('main-content');
                        videoPlayerContents = videoPage.getElementsByClassName('videoPlayer');

                        // remove removeMe DIV
                        // simulating a small delay...
                        if(mainContents.length && videoPageMainContents.length && videoPlayerContents.length) {
                            mainContents[0].innerHTML = videoPageMainContents[0].innerHTML;
                            mainContents[0].className += ' videoRenderer';

                            // just be sure everything is actually written to the DOM before we close the loading
                            setTimeout(function(){
                                document.documentElement.className = document.documentElement.className.replace(' video-loading', '');
                                bodyTag.removeChild(document.getElementById('loading'));

                                var event = new CustomEvent('tmg:page-loaded', { bubbles: true, cancelable: true, detail: undefined });
                                document.dispatchEvent(event);
                            }, 0);
                        } else {
                            // remove playlist GET from URL by redirecting to the article
                            window.location.replace(window.location.origin + window.location.pathname);
                        }
                    }
                    // else status code != 200
                    else {
                        // remove playlist GET from URL by redirecting to the article
                        window.location.replace(window.location.origin + window.location.pathname);
                    }
                }
            }


            videoURL = window.location.protocol + '//' + window.location.host

            /*
            Making it work in local as well as on live
            article/?playlist=
            article.html?playlist=
            */

            // does it end in .html?
            if(window.location.pathname.indexOf('.html', window.location.pathname.length - 5) !== -1) {
                videoURL += window.location.pathname.replace(/\.html$/, '.video.html');
            } else {
                videoURL += window.location.pathname.replace(/\/$/, '.video/');
            }

            videoURL += (window.location.href.indexOf('wcmmode=disabled') != -1 ? '?wcmmode=disabled' : '')

            // tmg.GET_PARAM.playlist
            xmlhttp.open("GET", videoURL, true);
            xmlhttp.send();

        }
    })()
